{
  "env": {
    "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "camelcase": "off",
    "import/order": "off",
    "import/no-unresolved": "off",
    "import/extensions": [
      "error",
      "ignorePackages"
    ]
  }
}
